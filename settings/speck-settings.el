(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(speck-chunk-at-point (quote commands))
 '(speck-doublets t)
 '(speck-engine (quote Hunspell))
 '(speck-hunspell-coding-system (quote utf-8))
 '(speck-hunspell-default-dictionary-name "en")
 '(speck-hunspell-dictionary-alist (quote (("en" . "en_US") ("ru" . "~/stuff/dict/hunspell/ru_RU_ie"))))
 '(speck-hunspell-language-options (quote (("en" iso-8859-1 nil nil nil) ("ru" utf-8 nil nil nil))))
 '(speck-hunspell-library-directory "/usr/share/hunspell/"))
